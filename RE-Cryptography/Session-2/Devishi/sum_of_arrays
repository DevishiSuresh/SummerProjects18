:arraysum

section .data

array1 dw 3,6,9,18
array2 dw 9,108,24,21
array3 dw 0,0,0,0

section .text

global sum:

sum:

mov rax,array1 ;add of arra1 to rax
mov rbx,array2 ;add of arra2 to rbx
mov rcx,array3 ;add of arra3 to rcx

loop:
mov r9,0 ; as a counter 
cmp 4,r9 
je return
add rcx,[array1]
add rcx,[array2]
inc rcx
inc array1
inc array2
inc r9

return:
ret
